{
  "name": "bardan-io/simple-id",
  "description": "A simple and easy to use package to generate unique ids for your Eloquent models.",
  "type": "libarry",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "BardanIO\\SimpleId\\": "src/",
      "BardanIO\\SimpleId\\Tests\\": "tests/"
    }
  },
  "require": {
    "hashids/hashids": "^5.0@dev",
    "laravel/framework": "^11.0"
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^11.0.1",
    "orchestra/testbench": "^v9.0.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "BardanIO\\SimpleId\\SimpleIdServiceProvider"
      ]
    }
  }
}